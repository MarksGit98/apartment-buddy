{
  "manifest_version": 2,
  "name": "Zillow Buddy",
  "version": "0.001",
  "content_scripts": [
    {
      "matches": [
        "http://zillow.com/*",
        "https://zillow.com/*",
        "http://*.zillow.com/*",
        "https://*.zillow.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
